<?xml version="1.0" encoding="UTF-8"?>
<zk>
	<library-property>
		<name>org.zkoss.web.servlet.http.URLEncoder</name>
		<value>lan.test.portlet.zk.wsrp.encoder.LiferayProxiedPortletURLEncoder</value>
	</library-property>
	<client-config>
        <debug-js>true</debug-js>
    </client-config>
    <library-property>
        <name>org.zkoss.web.classWebResource.cache</name>
        <value>false</value>
    </library-property>
	<!--<library-property>
		<name>org.zkoss.zk.portlet2.namespacedParameter.enabled</name>
		<value>true</value>
	</library-property>-->
	<listener>
		<description>App init</description>
		<listener-class>lan.test.portlet.zk.util.UIUtils</listener-class>
	</listener>
	<system-config>
		<cache-provider-class>org.zkoss.zk.ui.impl.GlobalDesktopCacheProvider</cache-provider-class>
		<ui-factory-class>lan.test.portlet.zk.wsrp.session.DualSerializableUiFactory</ui-factory-class>
	</system-config>
</zk>